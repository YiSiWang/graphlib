{
  "name": "@antv/graphlib",
  "version": "1.2.0",
  "scripts": {
    "size": "limit-size",
    "test": "jest --config jest.config.js --coverage -i",
    "build:umd": "rimraf ./dist && rollup -c && npm run size",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build": "run-p build:*",
    "bundle-vis": "cross-env BUNDLE_VIS=1 run-p build:umd"
  },
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@types/jest": "^29.2.4",
    "cross-env": "^7.0.3",
    "jest": "^29.3.1",
    "limit-size": "^0.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^5.6.0",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "limit-size": [
    {
      "path": "dist/index.min.js",
      "limit": "10 Kb",
      "gzip": true
    }
  ],
  "homepage": "https://github.com/antvis/graphlib",
  "bugs": {
    "url": "https://github.com/antvis/graphlib/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/graphlib.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
